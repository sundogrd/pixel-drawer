{
  "cSpell.words": [
    "Droppable",
    "stylelint"
  ],
  "jest.pathToConfig": "./jest.config.js",
  "jest.pathToJest": "./node_modules/.bin/jest",
  "eslint.enable": true,
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.stylelint": true,
    "source.fixAll.eslint": true
  }
}
